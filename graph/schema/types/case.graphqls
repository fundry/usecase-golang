type Case {
    id: Int!
    name : String!
    author : String!
    bookmarked: Boolean! @default(value : false)
    integrations : [String]
    collaborators: [String]
    createdAt : Time!
    updatedAt : Time!
}

input NewCase  {
    name : String!
    author : String!
    bookmarked: Boolean
    integrations : [String]
    collaborators: [String]
}

input UpdateCase {
    name : String!
    bookmarked: Boolean
    integrations : [String]
    collaborators: [String]
}