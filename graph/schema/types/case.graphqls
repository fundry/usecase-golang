type Case@goModel(model:  "github.com/vickywane/usecase-server/graph/model.Case")
{
    id: ID!
    name : String!
    author : String

    createdAt : Time!
    updateAt : Time
}

input NewCase@goModel(model:  "github.com/vickywane/usecase-server/graph/model.NewCase") {
    id: String!
    name : String!
    author : String
}
