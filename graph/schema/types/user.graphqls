type User@goModel(model:  "github.com/vickywane/usecase-server/graph/model.User"){
    id: ID!
    name: String!
    email: String
    cases: [Case]
    usecase: [Usecase]
    createdAt : Time
}

input NewUser@goModel(model:  "github.com/vickywane/usecase-server/graph/model.NewUser") {
    id: String!
    name: String!
    email: String
    createdAt : Time
}

