type User {
    id: Int!
    name: String!
    email: String
    isOrganization: Boolean! @default(value : false)
    bokmarkedCases : [Case]
    bokmarkedUsecases : [Usecase]
    cases: [Case]
    usecase: [Usecase]
    createdAt : Time!
}

input NewUser {
    name: String!
    email: String
    isOrganization: Boolean
    cases: [NewCase]
    usecases: [NewUsecase]
    bokmarkedCases : [NewCase]
    bokmarkedUsecases : [NewUsecase]
}

