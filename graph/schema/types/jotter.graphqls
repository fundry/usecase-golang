type Jotter  {
    id : Int!
    usecase : String!
    index: Int!
    name : String!
    content : String!
    contributors  : [String!]
    completed : Boolean! @default(value: false)
    createdAt : Time!
    updatedAt : Time!
}

input NewJotter {
    index: Int!
    name : String!
    content : String!
    contributors  : [String!]
    usecase : String!
}