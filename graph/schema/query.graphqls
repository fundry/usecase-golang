type Query {
  user(id : Int!): [User]
  users: User!

  usecase(id : Int!): Usecase!
  usecases: [Usecase]!

  case(id : Int!): Case!
  cases: [Case]!

  jotter(id: Int!) : Jotter!
  jotters(usecase : String!) : Jotter!
  allJotters : [Jotter]!

  organization(id : Int!): Organization!
  organizations: [Organization]!
}
